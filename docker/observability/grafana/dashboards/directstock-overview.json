    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {"defaults": {"unit": "reqps"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "rps",
          "refId": "A"
        }
      ],
      "title": "HTTP RPS",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {"defaults": {"unit": "ms"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "title": "HTTP Latency p95",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {"defaults": {"unit": "percentunit"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) / clamp_min(sum(rate(http_requests_total[5m])), 0.001)",
          "legendFormat": "5xx rate",
          "refId": "A"
        }
      ],
      "title": "HTTP 5xx Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {"defaults": {"unit": "ms"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(directstock_db_query_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "db p95",
          "refId": "A"
        },
        {
          "expr": "sum(increase(directstock_db_slow_query_total[5m]))",
          "legendFormat": "slow queries /5m",
          "refId": "B"
        }
      ],
      "title": "DB Latency and Slow Queries",
      "type": "timeseries"
    }
  ],
