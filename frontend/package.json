{
  "name": "directstock-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "../scripts/run_e2e_isolated.sh",
    "test:e2e:raw": "sh -c 'playwright test --project=web-desktop --project=ios-iphone-se --project=ios-ipad \"$@\"; code=$?; rm -rf test-results playwright-report blob-report; exit $code' --",
    "test:e2e:responsive": "sh -c 'playwright test tests/e2e/shell-navigation-smoke.spec.ts --project=ios-iphone-se --project=ios-ipad; code=$?; rm -rf test-results playwright-report blob-report; exit $code'",
    "test:ui": "playwright test --ui",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.62.7",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "vite-plugin-pwa": "^1.2.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^4.0.18"
  }
}
